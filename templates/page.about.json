        "1660681373b2fc3441-1": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/Group_31.png",
            "title": "",
            "text": "<p>Mature spirits in days, not decades<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "1660681373b2fc3441-2": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/Group_32.png",
            "title": "",
            "text": "<p>Bring out unique tasting notes that alternatives couldn't<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "7d9b4381-1c9f-4ae2-a240-264be84bf152": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/Group_33.png",
            "title": "",
            "text": "<p>Improve flavor in a controlled <br\/>and measurable way<\/p>",
            "link_label": "",
            "link": ""
          }
        }
      },
      "block_order": [
