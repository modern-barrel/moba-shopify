    "166067882923f6ef01": {
      "type": "rich-text",
      "blocks": {
        "16606788299f4eec76-0": {
          "type": "heading",
          "settings": {
            "heading": "<p>The science of barrel maturation <\/p>",
            "heading_size": "h1"
          }
        },
        "16606788299f4eec76-1": {
          "type": "text",
          "settings": {
            "text": "<p>Four main processes occur in barrel maturation to produce the flavors we all love.<\/p>"
          }
        }
      },
      "block_order": [
        "16606788299f4eec76-0",
        "16606788299f4eec76-1"
      ],
      "settings": {
